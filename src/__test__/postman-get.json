        "url": {
          "raw": "http://localhost:8000/housing/dot?select=Dimension.Value,CSDUID&where=CSD&value=Red%20Deer&string=true",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["housing", "dot"],
          "query": [
            { "key": "select", "value": "Dimension.Value,CSDUID" },
